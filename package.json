{
  "name": "prisma-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for GraphQL server built with Express and Apollo",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon -e js,graphql -x node --inspect src/index.js",
    "test": "cross-env NODE_ENV=test jest --watchAll",
    "db:drop": "prisma reset -e .env -f",
    "db:deploy": "prisma deploy -e .env -f",
    "db:info": "prisma info -e .env -j",
    "db:seed": "npm run db:drop && prisma seed -e .env",
    "db:token": "prisma token -e .env -c"
  },
  "keywords": [
    "prisma",
    "graphql",
    "apollo",
    "express"
  ],
  "author": {
    "name": "benjaminadk",
    "email": "benjaminadk@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "2.14.2",
    "axios": "0.19.0",
    "bcryptjs": "2.4.3",
    "connect-redis": "3.4.2",
    "cross-env": "5.2.0",
    "date-fns": "2.0.0-beta.4",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "express-rate-limit": "5.0.0",
    "express-session": "1.16.2",
    "graphql": "14.4.2",
    "graphql-import": "0.7.1",
    "graphql-resolvers": "0.3.3",
    "ioredis": "4.14.0",
    "jsonwebtoken": "8.5.1",
    "md5": "2.2.1",
    "nodemailer": "6.3.0",
    "passport": "0.4.0",
    "passport-twitter": "1.0.4",
    "prisma-client-lib": "1.34.3",
    "rate-limit-redis": "1.6.0"
  },
  "devDependencies": {
    "jest": "24.8.0",
    "nodemon": "1.19.1"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?)$",
    "moduleFileExtensions": [
      "js"
    ]
  }
}
